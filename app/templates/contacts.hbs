<div class="row-fluid">
  <div class="col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">
          Contacts
        </h3>
      </div>
      <div class="panel-body">
        <ul>
          {{#each model as |contact|}}
            <li>
              {{#link-to 'contacts.contact' contact}}
                {{contact.nameFirst}} {{contact.nameLast}}
              {{/link-to}}
            </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
  <div class="col-md-6">
    {{outlet}}
  </div>
</div>
