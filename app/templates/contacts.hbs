<div class="row-fluid">
  <div class="col-md-6">
    <ul>
      {{#each model as |contact|}}
        <li>
          {{#link-to 'contacts.contact' contact}}
            {{contact.nameFirst}} {{contact.nameLast}}
          {{/link-to}}
        </li>
      {{/each}}
    </ul>
  </div>
  <div class="col-md-6">
    {{outlet}}
  </div>
</div>
