<div class="form-group">
  <label>
    First Name
  </label>
  <input value={{contact.nameFirst}} oninput={{action (mut contact.nameFirst)
  value="target.value"}} class="form-control">
</div>
<div class="form-group">
  <label>
    Last Name
  </label>
  <input value={{contact.nameLast}} oninput={{action (mut contact.nameLast)
  value="target.value"}} class="form-control">
</div>
<div class="form-group">
  <label>
    Email
  </label>
  <input value={{contact.email}} oninput={{action (mut contact.email)
  value="target.value"}} class="form-control">
</div>
<div class="form-group">
  <label>
    Twitter
  </label>
  <input value={{contact.twitter}} oninput={{action (mut contact.twitter)
  value="target.value"}} class="form-control">
</div>
{{#bs-button action='save'}}
Save
{{/bs-button}}
